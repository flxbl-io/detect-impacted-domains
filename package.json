{
  "name": "@flxbl-io/detect-impacted-domains",
  "version": "1.0.0",
  "description": "GitHub Action to detect impacted domains based on git changes",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build src/index.ts -o dist --source-map --minify",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "all": "npm run format && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flxbl-io/detect-impacted-domains.git"
  },
  "keywords": [
    "github-action",
    "sfp",
    "salesforce",
    "release",
    "domains",
    "git",
    "diff",
    "flxbl"
  ],
  "author": "flxbl-io",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/flxbl-io/detect-impacted-domains/issues"
  },
  "homepage": "https://github.com/flxbl-io/detect-impacted-domains#readme",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "glob": "^10.3.10",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20"
  }
}
